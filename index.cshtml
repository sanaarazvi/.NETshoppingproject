@model List<string>

<h2 class="text-center mb-3">Your Previous Orders</h2>

@if (Model.Count == 0)
{
    <p class="text-center">No orders found.</p>
}
else
{
    <ul style="width:90%; margin:auto; font-size:18px;">
        @foreach (var order in Model)
        {
            <li style="padding:8px; border-bottom:1px solid #ccc;">@order</li>
        }
    </ul>
}
